[DEFAULT]
devices = /srv/node<%= @num %>
mount_check = false
disable_fallocate = true
bind_port = 60<%= @num %>0
user = swift
log_facility = LOG_LOCAL<%= @num %>
recon_cache_path = /etc/swift

[pipeline:main]
pipeline = recon object-server

[app:object-server]
use = egg:swift#object

[filter:recon]
use = egg:swift#recon

[object-replicator]
vm_test_mode = yes

[object-updater]

[object-auditor]
